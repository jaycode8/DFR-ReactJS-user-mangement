
{
    "builds":[{
        "src": "Backend/wsgi.py",
        "use": "@vercel/python",
        "config": {"maxLambdaSize": "15mb", "runtime": "python3"},
        "build":{
            "env":{
                "PYTHONUNBUFFERED":"1"
            },
            "commands":{
                "build":"sudo apt-get install -y libmysqlclient-dev && pip install -r requirements.txt"
            }
        }
    }],
    "routes":[{
        "src": "/(.*)",
        "dest": "Backend/wsgi.py"
    }]
}
